<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Workout Tracker</title>
</head>
<body>
  <h1>Workout Tracker</h1>

    <form id="workout-form">
      <input type="text" id="name" placeholder="Workout">
      <button id="add-workout" type="submit">Enter</button>
    </form>

    <ul>

      <li id="workout-list">
        <span id="workout-name"></span>
        <span id="set1"></span>
        <span id="set2"></span>
        <span is="set3"></span>
      </li>

    </ul>



  <script src="/script.js"></script>
</body>
</html>